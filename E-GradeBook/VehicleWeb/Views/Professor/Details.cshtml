@model Domain.SubjectProfessor



<h3>@Model.Professor.FirstName - @Model.Subject.Name</h3>

<a asp-action="Index">Врати се назад</a>

<table class="table">
    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>

        </tr>
    </thead>
    <tbody>

        @for (int i = 0; i < Model.ProfessorStudents.Count(); i++)
        {
            var item = Model.ProfessorStudents.ElementAt(i);
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Student.FirstName)

                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Student.LastName)
                </td>
                <td>
                <a class="btn btn-info" asp-controller="Professor" asp-action="Grades" asp-route-SubjectId="@Model.SubjectId"
                   asp-route-StudentId="@item.Student.Id">
                    Оценки
                </a>
                </td>
            </tr>
        }
        }

    </tbody>
</table>